<Application x:Class="WpfDiploma.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <BitmapImage x:Key="StartImageSource" UriSource="Start.ico"></BitmapImage>
        <BitmapImage x:Key="StopImageSource" UriSource="Stop.ico"></BitmapImage>
        <BitmapImage x:Key="PauseImageSource" UriSource="Pause.ico"></BitmapImage>
        <BitmapImage x:Key="SaveImageSource" UriSource="Save.ico"></BitmapImage>
        <BitmapImage x:Key="LoadImageSource" UriSource="Load.png"></BitmapImage>
        <BitmapImage x:Key="Advection1ImageSource" UriSource="advection1.png"></BitmapImage>
        <BitmapImage x:Key="Advection2ImageSource" UriSource="advection2.png"></BitmapImage>
    </Application.Resources>
</Application>
